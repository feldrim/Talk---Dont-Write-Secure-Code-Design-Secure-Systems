@using SecureFrameworkDemo.Models;
@{
	ViewBag.Title = "Order List (Insecure)";

	var orders = (IList<InsecureOrderViewModel>)(ViewBag.Orders);
}

<h2>@ViewBag.Title</h2>

<table class="table">
	<tr>
		<td>Id</td>
		<td>Placed On</td>
		<td>Customer</td>
		<td>Customer SSN</td>
		<td></td>
	</tr>
	@foreach (var order in orders) {
		<tr>
			<td>@order.Id</td>
			<td>@order.PlacedOn</td>
			<td>@order.UserName</td>
			<td>@order.SSN</td>
			<td>@Html.ActionLink("View Details", "OrderDetail", "Insecure", new { id = order.Id }, null)</td>
		</tr>
	}
</table>
